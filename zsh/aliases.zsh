# Aliases for zsh 
# Abhinav <abhixec@abhixec.com>

alias zmv="zmv -wM"
alias zcp="zmv -wC"
alias zln="zmv -wL"

# Open the zshrc
alias zrc="vim ~/.zshrc"
alias zrce="emacs ~/.zshrc"
alias zre="source ~/.zshrc"

# Git aliases
alias gd="git diff --"
alias gss="git status --short"
alias gco="git checkout"
alias ga="git add"
alias gc="git commit"
alias gcm="git commit -m "
alias g="git"
alias gp="git pull"
alias gst="git stash save"
